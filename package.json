  "scripts": {
    "build": "rimraf public && npm run compile-to-public",
    "build:debug": "nodemon -L --watch ./public --inspect=0.0.0.0:$DEBUG_PORT --ignore node_modules public/main/http/express/server.js",
    "build:server": "nodemon --ignore node_modules public/main/http/express/server.js",
    "build:watch": "rimraf public && npm run compile-to-public:watch",
    "compile-to-public": "tsc -p tsconfig-build.json",
    "compile-to-public:watch": "npm run compile-to-public -- --w",
    "sonar:scanner": "npm run test:ci && sonar-scanner",
    "test": "cross-env NODE_ENV=test cross-env REPOSITORY_TYPE=Memory jest --passWithNoTests --noStackTrace --runInBand --no-cache --detectOpenHandles",
    "test:ci": "npm run test --coverage",
    "test:staged": "npm run test --silent",    
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "amqplib": "^0.6.0",
    "bcrypt": "^5.0.0",
    "cross-env": "^7.0.3",
